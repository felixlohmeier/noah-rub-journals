                    <titleInfo lang="eng">
                        <title>Constraints on the development of liberal ideas and practices in colonial Namibia</title>
                    </titleInfo>
                    <name type="personal">
                        <displayForm>Botha, Christo</displayForm>
                        <namePart type="family">Botha</namePart>
